

#################################################
# Read string function
#################################################

ml_read_string

	PUSH -3[0]
	CONSTI -1
	ADDI
rs_l1	CONSTI 1
	ADDI
	DUP
	READC
	DUP
	CONSTI 10
	EQI
	IFNZ rs_done
	POPI
	GOTO rs_l1
rs_done	ADJUST -1
	CONSTI 0
	POPI
	ADJUST -1
	RET 0

