################################################################################
# sqr(x) - returns the square of x
################################################################################

ml_sqr
	# x * x
	PUSH	-3[0]
	PUSH	-3[0]
	MULR

	POP	-3[0]
	RET	0

ml_sqr_int
	# x * x
	PUSH	-3[0]
	PUSH	-3[0]
	MULI

	POP	-3[0]
	RET	0

