program btest007(input, output);
{ Test Case: can't have a literal newline in a string literal }
var
        str : string;

begin
        str := 'asdghkajsdghjkadshakjsdghkajsdgj\n\n\n\n\t\n\n\n\n
               ahsdajdhgajsdhashdjadg
               sdklfjslkdfja;skdjlkj
               ksdjflskdjflskdjfkldsjfklsjfkldjf';
        writeln(str);
end.
